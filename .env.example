# AerialDeck Configuration

# Your Dronedeck API token (get from app.dronedeck.io/user/api-tokens)
DRONEDECK_API_TOKEN=your_api_token_here

# Password to access AerialDeck
DASHBOARD_PASSWORD=your_secure_password

# Session secret (use a random string)
SESSION_SECRET=your_random_session_secret

# Port (optional, defaults to 3000)
PORT=3000
